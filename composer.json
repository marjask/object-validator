{
    "name": "marjask/object-validator",
    "description": "A simple and flexible validator that verifies the correctness of objects.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Marjask\\ObjectValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ObjectValidator\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Mariusz"
        }
    ],
    "require": {
      "ext-mbstring": "*",
      "php": ">=8.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.1",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "tests": [
            "php vendor/bin/phpunit Tests"
        ]
    }
}
